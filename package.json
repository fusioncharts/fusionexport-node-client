    "https": "^1.0.0",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.15",
    "minify": "^6.0.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.3.1",
    "sync-rpc": "^1.3.6",
    "tmp": "0.0.33",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^4.8.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "mocha": "^5.2.0",
    "prettier": "^1.19.1"
